  PU_TiposOrigen    PU_TiposOrigen @relation(fields: [origenInformacion], references: [id], onUpdate: NoAction, map: "FK__PU_Direcc__orige__6D0D32F4")
}

model PU_Documentos {
  id                String            @id(map: "PK__PU_Docum__3213E83F212D22A0") @default(dbgenerated("newid()"), map: "DF__PU_Documento__id__52593CB8") @db.UniqueIdentifier
