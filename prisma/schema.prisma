generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model IC_Cobrador {
  idCobrador     BigInt      @id(map: "PK__IC_Cobra__928D6DB7EEEACF1A")
  Nombre         String?     @db.VarChar(100)
  Usuario        String?     @db.VarChar(50)
  estado         Boolean?
  CoNombre       String?     @db.Char(50)
  CoApellido     String?     @db.Char(50)
  Password       Bytes?      @db.Binary(50)
  superusuario   Boolean?
  email          String?     @db.VarChar(50)
  NombreCompleto String?     @db.VarChar(150)
  PU_Perfil      PU_Perfil[]
}

model PU_Contactos {
  id                String           @id(map: "PK__PU_Conta__3213E83FBEB4A006") @default(dbgenerated("newid()"), map: "DF__PU_Contactos__id__01142BA1") @db.UniqueIdentifier
  nombreCompleto    String           @db.VarChar(150)
  origenInformacion Int
  tipoContacto      Int
  idPerfil          String           @db.UniqueIdentifier
  telefono          String?          @db.VarChar(15)
  PU_Perfil         PU_Perfil        @relation(fields: [idPerfil], references: [id], onUpdate: NoAction, map: "FK__PU_Contac__idPer__02084FDA")
  PU_TiposOrigen    PU_TiposOrigen   @relation(fields: [origenInformacion], references: [id], onUpdate: NoAction, map: "FK__PU_Contac__orige__02FC7413")
  PU_TiposContacto  PU_TiposContacto @relation(fields: [tipoContacto], references: [id], onUpdate: NoAction, map: "FK__PU_Contac__tipoC__03F0984C")
}

model PU_Correos {
  id                String         @id(map: "PK__PU_Corre__3213E83F80E0DACF") @default(dbgenerated("newid()"), map: "DF__PU_Correos__id__571DF1D5") @db.UniqueIdentifier
  correo            String         @db.VarChar(150)
  idPerfil          String         @db.UniqueIdentifier
  origenInformacion Int
  PU_Perfil         PU_Perfil      @relation(fields: [idPerfil], references: [id], onUpdate: NoAction, map: "FK__PU_Correo__idPer__5812160E")
  PU_TiposOrigen    PU_TiposOrigen @relation(fields: [origenInformacion], references: [id], onUpdate: NoAction, map: "FK__PU_Correo__orige__59063A47")
}

model PU_Direcciones {
  id                String         @id(map: "PK__PU_Direc__3213E83F7FD3C01A") @default(dbgenerated("newid()"), map: "DF__PU_Direccion__id__6B24EA82") @db.UniqueIdentifier
  departamento      String         @db.VarChar(50)
  municipio         String         @db.VarChar(50)
